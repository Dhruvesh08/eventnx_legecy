@model IEnumerable<LinkedInDemo.Models.EventMasterModel>
@using DataLayer
@{
    Layout = null;
}

<div id="grddata">
    <div class="col-md-12 col-sm-12 padrightzero">

        <div class="searchareamain">
            <ul>
                <li class="min_width250">
                    <input name="txtSearchTitle" type="text" maxlength="100" class="form-control" id="txtSearchTitle" style="width: 100%;" placeholder="Search">
                    <button id="searchid" onclick="GetData(1,@ViewBag.CurrentPageIndex,'@ViewBag.OrderByVal',@ViewBag.IsAscVal,@ViewBag.PageSize,'',@ViewBag.SearchRecords,'','@ViewBag.Domain','@ViewBag.Event')">x</button>
                </li>

                <li>  <input type="submit" name="btnSearch" value="Search" id="btnSearch" onclick="javascript:FunSearchRecord();" class="btn btnRed"></li>
            </ul>
        </div>
    </div>
    <div></div>
    <div class="row">
        <div class="col-sm-12">
            <div class="pad-top">
                @if (ViewBag.StatusMessage.ToString() == "SuccessAdd")
                {
                    <div class="alert alert-success margintop20">
                        <div class="tick">
                            Event Added Successfully.
                        </div>
                    </div>

                }
                @if (ViewBag.StatusMessage.ToString() == "SuccessUpdate")
                {
                    <div class="alert alert-success margintop20">
                        <div class="tick">
                            Event Updated Successfully.
                        </div>
                    </div>

                }
                @if (ViewBag.StatusMessage.ToString() == "SuccessDelete")
                {
                    <div class="alert alert-success margintop20">
                        <div class="tick">
                            Event Deleted Successfully.
                        </div>
                    </div>

                }
                @if (ViewBag.StatusMessage.ToString() == "formSaved")
                {
                    <div class="alert alert-success margintop20">
                        <div class="tick">
                            Event form saved successfully
                        </div>
                    </div>
                }
                @if (ViewBag.StatusMessage.ToString() == "Error")
                {

                    <div class=" alert alert-danger margintop20">
                        <div class="cross">
                            Error... Please Try Again.
                        </div>
                    </div>

                }
                @if (ViewBag.StatusMessage.ToString() == "NoItem")
                {
                    <div class=" alert alert-danger margintop20">
                        <div class="cross">
                            No records found
                        </div>
                    </div>

                }

            </div>
        </div>
    </div>
    <div class="row search-areamain marleftzero">
        <div class=" col-md-4 col-sm-5 padleftzero">
            @if (Model.Count() > 0)
            {
                <div class="center_div">
                    @{
                        ViewBag.TotalRecords = ViewBag.TotalEvent;
                        Html.RenderPartial("_PageSize");
                    }
                </div>

            }
        </div>
        @*<div class="col-md-8 col-sm-7 padrightzero">

                <div class="searchareamain">
                    <ul>
                            <li class="min_width250">
                                <input name="txtSearchTitle" type="text" maxlength="100" class="form-control" id="txtSearchTitle" style="width: 100%;" placeholder="Search">
                                <button id="searchid" onclick="GetData(1,@ViewBag.CurrentPageIndex,'@ViewBag.OrderByVal',@ViewBag.IsAscVal,@ViewBag.PageSize,'',@ViewBag.SearchRecords,'','@ViewBag.Domain','@ViewBag.Event')">x</button>
                            </li>

                            <li>  <input type="submit" name="btnSearch" value="Search" id="btnSearch" onclick="javascript:FunSearchRecord();" class="btn btnRed"></li>
                    </ul>
                </div>
            </div>*@

        <div class="clear"></div>

    </div>
    @if (Model.Count() > 0)
    {
        <div class="datatable">

            <div class="table-responsive">
                <table class="table borderbtm">
                    <tbody>
                        <tr>
                            <th><a onclick="javascript:FunSortData('EventName');">Event Details</a></th>
                            <th class="text-center"><a>Total Referrals</a></th>
                            <th class="text-center"><a onclick="javascript:FunSortData('RegisteredUsers');">Total Registrations</a></th>
                            @*<th class="text-center">LinkedIn Registrations</th>
                            <th class="text-center">Facebook Registrations</th>*@
                            <th width="26%" class="text-center">Actions</th>
                            <th class="text-center">Details</th>
                        </tr>
                        @foreach (var e in Model)
                        {
                            <tr>
                                <input type="hidden" id="downloadeventid" name="downloadeventid" value="@e.EventId" />

                                @if (e.ImageName == null || e.ImageName == "")
                                {
                                    <td>
                                        <div class="detail_and_img">
                                            <span class="img_round">
                                                <img src="/Content/images/img_eventlogo.jpg" width="50px" height="50px" />
                                            </span>
                                            <span>
                                                <b> @e.EventName</b><br /> <text style="font-weight:normal"> From: </text> @e.EventStartDate &nbsp;<text style="font-weight:normal"> <br>To: </text>@e.EventEndDate
                                            </span>
                                        </div>
                                    </td>
                                }
                                else
                                {
                                    <td>
                                        <div class="detail_and_img">
                                            <span class="img_round">
                                                <img src="@e.Image" alt="" width="50" height="auto">
                                            </span>
                                            <span>
                                                <b> @e.EventName</b><br /> <text style="font-weight:normal"> From: </text> @e.EventStartDate &nbsp;<text style="font-weight:normal"> <br>To: </text>@e.EventEndDate
                                            </span>
                                        </div>
                                    </td>
                                }
                                <td class="text-center">
                                    <center><a href=@Url.Action("Index", "ReferralUserReport", new { EventId = @e.EventId })>@e.Referralusers.ToString("#,##0")</a></center>
                                </td>
                                @if (e.RegisteredUsers > 0)
                                {
                                    <td class="text-center"><a href=@Url.Action("Index", "UserMaster", new { EventId = @e.EventId })>@e.RegisteredUsers.ToString("#,##0")</a></td>
                                }
                                else
                                {
                                    <td class="text-center"> @e.RegisteredUsers.ToString("#,##0")</td>
                                }
                                @*<td class="text-center">@e.Linkedinusers</td>
                                <td class="text-center">@e.Facebookusers</td>*@
                                <td class="text-center link_multi">
                                    <a href="@Url.Action("Edit", "Event", new { EventId = e.EventId })" data-toggle="tooltip" data-placement="top" title="Edit Event">Edit</a>&nbsp;
                                    <a onclick="ShowModel(@e.EventId);" data-toggle="modal" data-target="#eventCodeModal" data-placement="top" title="Generate Script">Code</a>&nbsp;
                                    <a onclick="SetEventId(@e.EventId,'@e.ButtonURL','@e.FbButtonURL','@e.GoogleButtonURL');" data-toggle="modal" data-target="#linkedinbuttonModal" data-placement="top" title="Linkedin Buttons">Buttons</a>&nbsp;
                                    <a onclick="Delete(@e.EventId);" data-toggle="tooltip" data-placement="top" title="Delete Event">Archive</a>
                                    @if (e.CustomerId == ViewBag.CustomerId)
                                    {
                                        <a href="@Url.Action("InviteUser", "Event", new { EventId = e.EventId })" data-toggle="tooltip" data-placement="top" title="Edit Event">Users</a>
                                    }

                                </td>
                                <td class="text-center">
                                    <a class="btn btnRed padd_set" href="@Url.Action("Index", "RegisteredUser", new { EventId = e.EventId })" class="on-default edit-row iconbtn" data-toggle="tooltip" data-placement="top" title="Details">Details</a>

                                </td>
                            </tr>

                        }
                    </tbody>
                </table>
            </div>

        </div>
    }
    <div class="row mar_top_15">
        @{
            Html.RenderPartial("_Pagination");
        }
    </div>
</div>

<div class="modal fade" id="eventCodeModal" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h3>Event code</h3>
            </div>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
            <div class="set_1a special_btn_area">
                <a onclick="CopyToClipboard()">Copy</a>
                <a data-toggle="modal" data-target="#ModelEmail" data-placement="top" title="Linkedin Buttons">Send Email</a>
                @*<a href="@Url.Action("DownloadcodePDF", "RegisteredUser", new { EventId = })">Download code</a>*@
                <a onclick="Downloadcode()"> Download Code</a>
            </div>
            <div class="modal-body" id="modelbody">

            </div>

        </div>
    </div>
</div>
<div class="modal fade" id="ModelEmail" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document" style="max-width:600px">
        <div class="modal-content">
            <div class="modal-header">
                <h3>Send Email To Developer</h3>
            </div>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
            <div class="modal-body" id="modelbutttonbody">
                <input type="hidden" name="CodeEventId" id="CodeEventId" />
                <div class="col-lg-12 col-md-12">
                    <div class="linkdinbox">

                        <div class="alert alert-success" id="emailsuccessmsgbox" style="display:none">
                            <div class="tick">
                                <label id="buttonsuccessmsg"></label>
                            </div>
                        </div>
                        <div id="divdeveloperemail" style="max-width:300px; width:100%;">
                            <div class="form-group">
                                <input type="text" id="txtemail" name="txtemail" class="form-control" placeholder="Please Enter Email" />
                                <span id="divemailvalidation" style="display:none; color:red"></span>
                            </div>
                            <div class="form-group">
                                <div class="set_tow_btn">
                                    <a class="btn btnRed" onclick="EmailToDeveloperSubmit()" style="color:#fff;">Submit</a>
                                    @*<a class="btn btnblack" onclick="EmailToDeveloperCancel()">Cancel</a>*@
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>

        </div>
    </div>
</div>
